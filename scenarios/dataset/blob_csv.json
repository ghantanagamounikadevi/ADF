{
	"name": "blob_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_blob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"param_container": {
				"type": "string",
				"defaultValue": "sales"
			},
			"param_folder": {
				"type": "string",
				"defaultValue": "sales"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().param_file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().param_folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().param_container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}