{
	"name": "Patient_source_csv_Staging",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_blob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"p_container": {
				"type": "string",
				"defaultValue": "sales"
			},
			"p_folder": {
				"type": "string",
				"defaultValue": "patient"
			},
			"p_file": {
				"type": "string",
				"defaultValue": "Client_data.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().p_file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().p_folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().p_container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "PatientID",
				"type": "String"
			},
			{
				"name": "Department",
				"type": "String"
			},
			{
				"name": "CheckupDate",
				"type": "String"
			},
			{
				"name": "HeartRate",
				"type": "String"
			},
			{
				"name": "BloodPressure",
				"type": "String"
			},
			{
				"name": "BloodSugar",
				"type": "String"
			}
		]
	}
}