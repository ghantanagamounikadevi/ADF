{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorybymounikag"
		},
		"ls_blob_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'ls_blob'"
		},
		"Blob_for_snowflake_withSASURL_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'Blob_for_snowflake_withSASURL'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorybymounikag_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_sasUri": {
						"value": "[parameters('ls_blob_sasUri')]"
					},
					"Blob_for_snowflake_withSASURL_sasUri": {
						"value": "[parameters('Blob_for_snowflake_withSASURL_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}